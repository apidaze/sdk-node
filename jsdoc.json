{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": "./",
    "includePattern": ".+\\.js(doc|x)?$",
    "exclude": [
      "./node_modules",
      "./test",
      "./**/*.test.js"
    ],
    "excludePattern": "(\/test\/|.test.js$)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  },
  "opts": {
    "template": "./node_modules/minami",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true
  }
}
